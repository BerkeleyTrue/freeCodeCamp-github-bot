{
  "name": "freecodecamp-github-bot",
  "version": "1.0.0",
  "description": "A github bot for freecodecamp's open source repos on GitHub",
  "main": "run-server.js",
  "scripts": {
    "start": "node run-server.js",
    "test": "./node_modules/.bin/babel-node node_modules/mocha/bin/_mocha --recursive -R spec",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/istanbul/lib/cli cover -- node_modules/mocha/bin/_mocha --recursive --reporter spec"
  },
  "engines": {
    "node": ">=4"
  },
  "config": {},
  "keywords": [
    "Github",
    "bot"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "babel-cli": "^6.10.0",
    "babel-core": "^6.0.0",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-plugin-transform-regenerator": "^6.1.4",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-polyfill": "^6.1.4",
    "babel-preset-es2015": "^6.6.0",
    "body-parser": "^1.15.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "github": "^6.1.0",
    "secure-compare": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.1.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freeCodeCamp/camper-gh-bot.git"
  },
  "bugs": {
    "url": "https://github.com/freeCodeCamp/camper-gh-bot/issues"
